{
  "ci": {
    "collect": {
      "url": ["http://localhost:5173/"],
      "startServerCommand": "npm run dev",
      "numberOfRuns": 1
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "assertions": {
        "categories:accessibility": [
          "pass",
          { "minScore": 0.9 }
        ]
      }
    }
  }
}
